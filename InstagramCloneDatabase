mysql> select emp_no,department,Salary,
    -> Rank() over(order by Salary desc) as Over_All_Rank,
    -> dense_Rank() over(order by Salary desc) as Dept_Rank,
    -> Row_Number() over(Order by Salary desc) as Current_Row
    -> from employees;
